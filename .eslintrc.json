{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-console": [
      "error",
      {
        "allow": ["warn", "error"]
      }
    ],
    "no-warning-comments": [
      "error",
      {
        "terms": ["todo", "fixme", "xxx"],
        "location": "anywhere"
      }
    ],
    "no-debugger": "error"
  }
}
